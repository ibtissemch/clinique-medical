
@{
    ViewBag.Title = "DemandeForm";
    Layout = "~/Views/Shared/_layoutpatient.cshtml";
}
    <html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description" content="">
        <meta name="author" content="Dashboard">
        <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
        <title>Dashio - Bootstrap Admin Template</title>

        <!-- Favicons -->
        <link href=@Url.Content("/Content/img/favicon.png") rel="icon">

        <!-- Bootstrap core CSS -->
        <link href=@Url.Content("/Content/lib/bootstrap/css/bootstrap.min.css") rel="stylesheet">
        <!--external css-->
        <link href=@Url.Content("/Content/lib/font-awesome/css/font-awesome.css") rel="stylesheet" />
        <link rel="stylesheet" type="text/css" href=@Url.Content("/Content/lib/bootstrap-datepicker/css/datepicker.css") />
        <link rel="stylesheet" type="text/css" href=@Url.Content("/Content/lib/bootstrap-daterangepicker/daterangepicker.css") />
        <link rel="stylesheet" type="text/css" href=@Url.Content("/Content/lib/bootstrap-timepicker/compiled/timepicker.css") />
        <link rel="stylesheet" type="text/css" href=@Url.Content("/Content/lib/bootstrap-datetimepicker/datertimepicker.css") />
        <!-- Custom styles for this template -->
        <link href=@Url.Content("/Content/css/style.css") rel="stylesheet">
        <link href=@Url.Content("/Content/cssdashpat/style-responsive.css") rel="stylesheet">

        <!-- =======================================================
    ======================================================= -->
    </head>
    <body>
        <form method="post" action="DemandeForm">
            <div class="form-group">
                <label class="col-sm-5 col-sm-5 control-label">Nom Et Prénom</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" name="nometprenom">
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-5 col-sm-5 control-label">Numéro de Carnet CNSS</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" name="numcnss">
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-2 col-sm-2 control-label">Date</label>
                <div class="col-sm-10">
                    <input class="form-control form-control-inline input-medium default-date-picker" size="16" type="date" value="" name="date">
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-2 col-sm-2 control-label">Heure</label>
                <div class="col-sm-10">
                    <div class="input-group bootstrap-timepicker">
                        <input type="time" class="form-control timepicker-default" name="heure">
                        <span class="input-group-btn">
                            <button class="btn btn-danger" type="button"><i class="fa fa-clock-o"></i></button>
                        </span>
                    </div>
                </div>
            </div>
            <button type="submit" class="btn btn-outline-primary">Envoyer la demande</button>
        </form>

        <!-- js placed at the end of the document so the pages load faster -->
        <script src=@Url.Content("/Content/lib/jquery/jquery.min.js") type="text/javascript"></script>
        <script src=@Url.Content("/Content/lib/bootstrap/js/bootstrap.min.js") type="text/javascript"></script>
        <script class="include" src=@Url.Content("/Content/lib/jquery.dcjqaccordion.2.7.js") type="text/javascript"></script>
        <script src=@Url.Content("/Content/lib/jquery.scrollTo.min.js") type="text/javascript"></script>
        <script src=@Url.Content("/Content/lib/jquery.nicescroll.js") type="text/javascript"></script>
        <!--common script for all pages-->
        <script src=@Url.Content("/Content/lib/common-scripts.js") type="text/javascript"></script>
        <!--script for this page-->
        <script src=@Url.Content("/Content/lib/jquery-ui-1.9.2.custom.min.js") type="text/javascript"></script>
        <!--custom switch-->
        <script src=@Url.Content("/Content/lib/bootstrap-switch.js") type="text/javascript"></script>
        <!--custom tagsinput-->
        <script src=@Url.Content("/Content/lib/jquery.tagsinput.js") type="text/javascript"></script>
        <!--custom checkbox & radio-->
        <script src=@Url.Content("/Content/lib/bootstrap-datepicker/js/bootstrap-datepicker.js") type="text/javascript"></script>
        <script src=@Url.Content("/Content/lib/bootstrap-daterangepicker/date.js") type="text/javascript"></script>
        <script src=@Url.Content("/Content/lib/bootstrap-daterangepicker/daterangepicker.js") type="text/javascript"></script>
        <script src=@Url.Content("/Content/lib/bootstrap-inputmask/bootstrap-inputmask.min.js") type="text/javascript"></script>
        <script src=@Url.Content("/Content/lib/form-component.js") type="text/javascript"></script>
        <script  src=@Url.Content("lib/bootstrap-fileupload/bootstrap-fileupload.js") type="text/javascript"></script>

        <script src=@Url.Content("/Content/lib/bootstrap-datetimepicker/js/bootstrap-datetimepicker.js") type="text/javascript"></script>
        <script src=@Url.Content("/Content/lib/bootstrap-daterangepicker/moment.min.js") type="text/javascript"></script>
        <script src=@Url.Content("/Content/lib/bootstrap-timepicker/js/bootstrap-timepicker.js") type="text/javascript"></script>
    </body>
    </html>