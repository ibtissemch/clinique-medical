
@{
    ViewBag.Title = "Connecter";

}
    <html>
    <head>
        <title>se connecter</title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <!--===============================================================================================-->
        <link rel="icon" type="image/png" href=@Url.Content("/Content/images/icons/favicon.ico" ) />
        <!--===============================================================================================-->
        <link rel="stylesheet" type="text/css" href=@Url.Content("/Content/vendor/bootstrap/css/bootstrap.min.css")>
        <!--===============================================================================================-->
        <link rel="stylesheet" type="text/css" href=@Url.Content("/Content/fonts/font-awesome-4.7.0/css/font-awesome.min.css")>
        <!--===============================================================================================-->
        <link rel="stylesheet" type="text/css" href=@Url.Content("/Content/fonts/iconic/css/material-design-iconic-font.min.css")>
        <!--===============================================================================================-->
        <link rel="stylesheet" type="text/css" href=@Url.Content("/Content/vendor/animate/animate.css")>
        <!--===============================================================================================-->
        <link rel="stylesheet" type="text/css" href=@Url.Content("/Content/vendor/css-hamburgers/hamburgers.min.css")>
        <!--===============================================================================================-->
        <link rel="stylesheet" type="text/css" href=@Url.Content("/Content/vendor/animsition/css/animsition.min.css")>
        <!--===============================================================================================-->
        <link rel="stylesheet" type="text/css" href=@Url.Content("/Content/vendor/select2/select2.min.css")>
        <!--===============================================================================================-->
        <link rel="stylesheet" type="text/css" href=@Url.Content("/Content/vendor/daterangepicker/daterangepicker.css")>
        <!--===============================================================================================-->
        <link rel="stylesheet" type="text/css" href=@Url.Content("/Content/css/util.css")>
        <link rel="stylesheet" type="text/css" href=@Url.Content("/Content/css/main.css")>
        <!--===============================================================================================-->
    </head>
    <body>


        <div class="container-login100" style="background-image:url('/Content/images/bg.jpg') ;">
            <div class="wrap-login100 p-l-55 p-r-55 p-t-80 p-b-30">
                <form class="login100-form validate-form">
                    <span class="login100-form-title p-b-37">
                        Se Connecter
                    </span>

                    <div class="wrap-input100 validate-input m-b-20" data-validate="Entrer username or email">
                        <input class="input100" type="text" name="username" placeholder="username ou email">
                        <span class="focus-input100"></span>
                    </div>

                    <div class="wrap-input100 validate-input m-b-25" data-validate="Entrer password">
                        <input class="input100" type="password" name="pass" placeholder="mot de pass">
                        <span class="focus-input100"></span>
                    </div>

                    <div class="wrap-input100 validate-input m-b-20" data-validate="Choisir une fonction">
                        <select name="connect" class="input100">
                            <option value="directeur">Directeur Clinique</option>
                            <option value="chef">chef Service</option>
                            <option value="med">Medecin</option>
                            <option value="recep">Receptionniste</option>
                            <option value="patient">Patient</option>
                        </select>

                    </div>
                    <div>

                    </div>

                    <div class="container-login100-form-btn">
                        <button class="login100-form-btn">
                            Se Connecter
                        </button>
                    </div>



                </form>


            </div>
        </div>



        <div id="dropDownSelect1"></div>

        <!--===============================================================================================-->
        <script src=@Url.Content("/Content/vendor/jquery/jquery-3.2.1.min.js")></script>
        <!--===============================================================================================-->
        <script src=@Url.Content("/Content/vendor/animsition/js/animsition.min.js")></script>
        <!--===============================================================================================-->
        <script src=@Url.Content("/Content/vendor/bootstrap/js/popper.js")></script>
        <script src=@Url.Content("/Content/vendor/bootstrap/js/bootstrap.min.js")></script>
        <!--===============================================================================================-->
        <script src=@Url.Content("/Content/vendor/select2/select2.min.js")></script>
        <!--===============================================================================================-->
        <script src=@Url.Content("/Content/vendor/daterangepicker/moment.min.js")></script>
        <script src=@Url.Content("/Content/vendor/daterangepicker/daterangepicker.js")></script>
        <!--===============================================================================================-->
        <script src=@Url.Content("/Content/vendor/countdowntime/countdowntime.js")></script>
        <!--===============================================================================================-->
        <script src=@Url.Content("/Content/js/main.js")></script>

    </body>

 </</html>