        <!-- Header section end -->
        <!-- Contact section -->
        <section class="contact-section spad">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="contact-text">
                            <h3>Contactez Nous </h3>
                            <div class="contact-info">
                                <div class="ci-image set-bg" data-setbg="/content/img/doctors/1.jpg"></div>
                                <div class="ci-content">
                                    <p>5019 Avenue Avicenne, Monastir</p>
                                    <p>73 460 832</p>
                                    <p>CliniqueDentaireMonastir@gmail.com</p>
                                </div>
                            </div>
                        </div>
                        <form class="contact-form" method="post" action="Form">
                            <input type="text" placeholder="Nom et Prénom" name="namePatient">
                            <input type="text" placeholder="E-mail" name="receiverEmail">
                            <input type="text" placeholder="Sujet" name="subject">
                            <textarea placeholder="Message" name="message"></textarea>
                            <button class="site-btn" type="submit">Envoyer un E-mail</button>
                        </form>
                    </div>
                </div>
            </div>
            <div class="map" id="map">
                <script>
                    var map;
                    function initMap() {
                        map = new google.maps.Map(document.getElementById('map'), {
                            center: { lat: 35.7644, lng: 10.8287 },
                            zoom: 8
                        });
                    }
                </script>
                <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCvjJ-K2tKSgEOoMUOWmt9oTkzMTiQM9pQ&callback=initMap"
                        async defer></script>
            </div>
